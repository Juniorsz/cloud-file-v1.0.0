RewriteEngine On

# Home
RewriteRule ^home/?$ index.php [L]

# Files
RewriteRule ^files/(.*)/?$ files.php?id=$1 [QSA,L]

# Download
RewriteRule ^download?$ download.php [L]

# Admin
RewriteRule ^admin/?$ admin.php [L]
RewriteRule ^admin/(.*)/?$ admin.php?page=$1 [QSA,L]

# Install
RewriteRule ^install/?$ install.php [L]
